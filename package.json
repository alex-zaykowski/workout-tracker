    "@rivascva/react-native-code-editor": "^1.2.2",
    "@types/react-native": "~0.70.6",
    "expo": "~47.0.12",
    "expo-sqlite": "~11.0.0",
    "expo-status-bar": "~1.4.2",
    "jest": "^29.0.0",
    "jest-expo": "^48.0.2",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-swipe-gestures": "^1.0.5",
    "ts-jest": "^29.1.0",
    "typescript": "^4.6.3",
